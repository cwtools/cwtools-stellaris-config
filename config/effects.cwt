	days = int
	## cardinality = 0..1
	months = int
	## cardinality = 0..1
	random = int
	## cardinality = 0..1
	scopes = {
		## cardinality = 0..1
		from = scope[any]
		## cardinality = 0..1
		fromfrom = scope[any]
		## cardinality = 0..1
		fromfromfrom = scope[any]
		## cardinality = 0..1
		fromfromfromfrom = scope[any]
	}
}

